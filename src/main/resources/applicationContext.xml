<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="t1000" class="org.pantheon.ModelT1000">
        <constructor-arg ref="sonyHead"/>
        <constructor-arg ref="toshibaHand"/>
        <constructor-arg ref="sonyLeg"/>
    </bean>

    <bean id="sonyHead" class="org.pantheon.sony.SonyHead"/>
    <bean id="sonyHand" class="org.pantheon.sony.SonyHand"/>
    <bean id="sonyLeg" class="org.pantheon.sony.SonyLeg"/>

    <bean id="toshibaHead" class="org.pantheon.toshiba.ToshibaHead"/>
    <bean id="toshibaHand" class="org.pantheon.toshiba.ToshibaHand"/>
    <bean id="toshibaLeg" class="org.pantheon.toshiba.ToshibaLeg"/>

</beans>